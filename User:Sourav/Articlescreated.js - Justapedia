</span><span id="L-47"><a href="https://justapedia.org/wiki/User:Sourav/Articlescreated.js#L-47"><span class="linenos" data-line="47"></span></a>    <span class="k">if</span> <span class="p">(</span><span class="nx">mw</span><span class="p">.</span><span class="nx">config</span><span class="p">.</span><span class="nx">get</span><span class="p">(</span><span class="s1">'wgNamespaceNumber'</span><span class="p">)</span> <span class="o">===</span> <span class="mf">2</span><span class="p">)</span> <span class="p">{</span>
</span><span id="L-48"><a href="https://justapedia.org/wiki/User:Sourav/Articlescreated.js#L-48"><span class="linenos" data-line="48"></span></a>        <span class="kd">var</span> <span class="nx">username</span> <span class="o">=</span> <span class="nx">mw</span><span class="p">.</span><span class="nx">config</span><span class="p">.</span><span class="nx">get</span><span class="p">(</span><span class="s1">'wgTitle'</span><span class="p">).</span><span class="nx">split</span><span class="p">(</span><span class="s1">'/'</span><span class="p">)[</span><span class="mf">0</span><span class="p">];</span> <span class="c1">// Get the username from the page title</span>
</span><span id="L-49"><a href="https://justapedia.org/wiki/User:Sourav/Articlescreated.js#L-49"><span class="linenos" data-line="49"></span></a>        <span class="nx">mw</span><span class="p">.</span><span class="nx">loader</span><span class="p">.</span><span class="nx">using</span><span class="p">([</span><span class="s1">'mediawiki.api'</span><span class="p">],</span> <span class="kd">function</span><span class="p">()</span> <span class="p">{</span>
</span><span id="L-50"><a href="https://justapedia.org/wiki/User:Sourav/Articlescreated.js#L-50"><span class="linenos" data-line="50"></span></a>            <span class="nx">addArticleCountToUserPage</span><span class="p">(</span><span class="nx">username</span><span class="p">);</span>
</span><span id="L-51"><a href="https://justapedia.org/wiki/User:Sourav/Articlescreated.js#L-51"><span class="linenos" data-line="51"></span></a>        <span class="p">});</span>
</span><span id="L-52"><a href="https://justapedia.org/wiki/User:Sourav/Articlescreated.js#L-52"><span class="linenos" data-line="52"></span></a>    <span class="p">}</span>
</span><span id="L-53"><a href="https://justapedia.org/wiki/User:Sourav/Articlescreated.js#L-53"><span class="linenos" data-line="53"></span></a><span class="p">})();</span>
</span></pre></div>
<!-- 
